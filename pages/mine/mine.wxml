<view class="container container-mine">
    <view class="mine">
        <view class="mine-info">
            <image class="mine-bg" src="../../image/mine/bg_1.jpg" />
            <view class="mine-info-head">
                <image class="mine-info-head-image" src="https://wx.qlogo.cn/mmopen/vi_32/a2G20RAuIMOjLBa9dDYfICADAIuqiciaM2gfpYr1jD1sgiaDITdYWFLAToXuzPbPFhhxEOjeAZxibq8lYoNgicDnunw/132" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload=""></image>
                <view class="fs30 f-color-fff lh2-5">姜欣欣</view>
                <view class="fs26 f-color-fff">(设计顾问)</view>
                <image class="qr-code" src="../../image/mine/icon_erwei.png" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload=""></image>
            </view>
        </view>
        <view class="entrance">
            <view class="entrance-list pt30" wx:for="{{entrance}}" wx:for-index="key" wx:for-item="item" wx:key="key">
                <view class="entrance-list-item lh2-5 pl40 pr40" wx:for="{{item}}" wx:for-item="value" wx:for-index="ind" wx:key="ind">
                    <view class="entrance-list-item-title">
                        <image class="entrance-list-item-image" src="{{value.icon}}" />
                        <view class="pl20 f-color-666">{{value.title}}</view>
                    </view>
                    <view class="entrance-list-item-right">
                        <image class="icon-right" src="../../image/common/icon_right.png" />
                    </view>
                </view>
            </view>
        </view>
        <view class="log-out pl40 pr40 pt40 pb40 mt40 fs30">
            <button class="log-out-button">退出登录</button>
        </view>
    </view>
</view>